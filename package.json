{
  "name": "node-ts-template",
  "version": "1.0.0",
  "description": "jsec's TypeScript starter kit",
  "main": "index.js",
  "scripts": {
    "prepare": "node ./scripts/prepare.js",
    "lint": "eslint --ext .ts . --max-warnings 0",
    "test": "ava",
    "test:coverage": "c8 ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsec/node-ts-template.git"
  },
  "keywords": [],
  "author": "Jarrod Seccombe <jarrod.seccombe@icloud.com>",
  "license": "MIT",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@jarsec/eslint-config": "2.0.2",
    "@jarsec/prettier-config": "1.2.0",
    "@jarsec/tsconfig": "1.1.0",
    "@types/node": "20.6.0",
    "ava": "5.3.1",
    "c8": "8.0.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "tsx": "3.12.8",
    "typescript": "5.2.2"
  }
}
